    "description": "Collection of React Hooks.",
    "license": "WTFPL",
    "main": "dist/lib/index.js",
    "module": "dist/es/index.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "https://github.com/nonoroazoro"
